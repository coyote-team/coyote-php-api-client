{
    "name": "coyote/coyote-api-client",
    "description": "A client for the Coyote.pics image description service",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "json-mapper/json-mapper": "^2.9.1",
        "php": ">=7.4",
        "ext-json": "*",
        "monolog/monolog": "2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "1.3.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Job van Achterberg",
            "email": "job@pac.bz"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Coyote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    }
}
